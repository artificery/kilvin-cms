{
    "name": "artificery/kilvin-cms",
    "authors": [
        {
            "name": "Paul Burdick",
            "email": "paul@reedmaniac.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "2.5.*",
        "erusev/parsedown": "^1.6",
        "ezyang/htmlpurifier": "^4.9",
        "laravel/framework": "5.7.*",
        "predis/predis": "1.0.*",
        "sentry/sentry-laravel": "^0.4.1",
        "twig/twig": "^2.0",
        "artificery/kilvin-docs": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "laravel/tinker": "^1.0",
        "filp/whoops": "^2.0",
        "nunomaduro/collision": "^2.0",
        "beyondcode/laravel-dump-server": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kilvin\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/images.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kilvin\\Tests\\": "src/tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kilvin\\Providers\\KilvinServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../kilvin-docs"
        }
    ]
}
