{
    "name": "artificery/kilvin-cms",
    "authors": [
        {
            "name": "Paul Burdick",
            "email": "paul@reedmaniac.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0.2",
        "erusev/parsedown": "^1.6",
        "ezyang/htmlpurifier": "^4.9",
        "laravel/framework": "^9.19",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "laravel/tinker": "^1.0",
        "filp/whoops": "^2.0",
        "nunomaduro/collision": "^2.0",
        "beyondcode/laravel-dump-server": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kilvin\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/images.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kilvin\\Tests\\": "src/tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kilvin\\Providers\\KilvinServiceProvider"
            ]
        }
    }
}
